public class Welcome{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int total=0;
        for(int i = 0 ; i<3;i++) {
            int a = sc.next();
            total += a;
        }
        string b = sc.next();
        System.out.println(a + "\n" + b);
    }
}